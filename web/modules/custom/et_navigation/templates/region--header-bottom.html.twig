{#
/**
 * @file
 * Theme override to display a region.
 * @see template_preprocess_region()
 */
#}
{%
  set classes = [
    'region',
    'region-' ~ region|clean_class,
    'megamenu',
  ]
%}
{% if content %}
  <nav{{ attributes.addClass(classes) }}>
    <ul class="nav-menu">
      {{ content }}
    </ul>
  </nav>
{% endif %}
